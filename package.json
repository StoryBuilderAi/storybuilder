{
  "name": "storybuilder",
  "version": "1.0.0",
  "description": "AI-powered Resume Analyzer and career coach . Includes seamless auth, upload and store resumes, and match candidates to jobs using smart AI evaluations.",
  "keywords": [
    "AI",
    "Resume",
    "Career"
  ],
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "homepage": "https://github.com/StoryBuilderAi/storybuilder#readme",
  "bugs": {
    "url": "https://github.com/StoryBuilderAi/storybuilder/issues"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "test": "turbo run test",
    "dev:server": "npm run dev --workspace=server",
    "dev:web": "npm run dev --workspace=web",
    "dev:client": "npm run dev --workspace=client",
    "build:server": "npm run build --workspace=server",
    "build:web": "npm run build --workspace=web",
    "build:client": "npm run build --workspace=client",
    "start:server": "npm run start --workspace=server",
    "start:web": "npm run start --workspace=web",
    "start:client": "npm run start --workspace=client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StoryBuilderAi/storybuilder.git"
  },
  "license": "ISC",
  "author": "https://github.com/wealthralph",
  "type": "commonjs",
  "main": "index.js",
  "dependencies": {
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
